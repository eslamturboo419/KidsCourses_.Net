@model KProj.Models.RestPasswordClass
@{
    ViewBag.Title = "RestPassword";
    Layout = "~/Views/Shared/_Layout1.cshtml";
}






<div class="change-settings">
    <div class="container">
        <div class="row">
            <div class="col-lg-4 col-12">
                <ul class="nav nav-tabs" role="tablist">

                    @if(ViewBag.Message !=null)
                    {
                        <li class="nav-item"> <p class="alert alert-success"> @ViewBag.Message   </p>  </li>
                    }
                    <li class="nav-item">
                        <a href="#personal-settings" aria-controls="personal-settings" class="active show" role="tab" data-toggle="tab">
                            Change Password
                        </a>
                    </li>
                </ul>
            </div>

            <div class="col-lg-8 col-12">
                <div class="tab-content">

                    <div class="tab-pane fade active show" role="tabpanel" id="personal-settings">
                        <div class="col-12">
                            <div class="settings-title">
                                Edit Your Password
                            </div>
                            <div class="personal-settings">

                                @using (Html.BeginForm())
    {
                            @Html.AntiForgeryToken()
                            @Html.HiddenFor(x => x.RestCode)

                            @*<form name="" action="" enctype="multipart/form-data">*@

                            <div class="full-name">
                                <label> New Password </label>
                                @*<input type="text" placeholder="Full Name" name="">*@
                                @Html.EditorFor(model => model.NewPassword, new { htmlAttributes = new { @class = "", placeholder = "New Password" } })

                            </div>

                            <div class="username">
                                <label> Confirm Password </label>
                                @*<input type="text" placeholder="User Name" name="">*@
                                @Html.EditorFor(model => model.ConfirmPassword, new { htmlAttributes = new { @class = " ", placeholder = "User Name" } })

                            </div>

                            <div class="button-submit">
                                <input type="submit" value="Save">
                            </div>
}
                                @*</form>*@
                            </div><!-- End personal Settings -->
                        </div><!-- End Column -->
                    </div>






                </div><!-- End tab Content -->
            </div>
        </div><!-- End Row -->
    </div><!-- End Container -->
</div>

<!-- Loader -->

<div class="preloader" id="preloader">
    <div class="loader loader-1">
        <div class="loader-outter"></div>
        <div class="loader-inner"></div>
    </div>
</div>





